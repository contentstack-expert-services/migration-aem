            "jcr:primaryType": "nt:unstructured",
            "text": "<p>&nbsp;</p>\r\n<h3 style=\"text-align: center;\">&nbsp;</h3>\r\n<h3 style=\"text-align: center;\"><span class=\"title3Heavy\">E-Bike Motor Options</span></h3>\r\n",
            "cq:responsive": {
              "jcr:primaryType": "nt:unstructured",
              "default": {
                "offset": "3",
                "width": "6",
                "jcr:primaryType": "nt:unstructured"
