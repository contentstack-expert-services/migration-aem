            "jcr:primaryType": "nt:unstructured",
            "text": "<p>&nbsp;</p>\r\n<p>&nbsp;</p>\r\n<h3 style=\"text-align: center;\"><span class=\"title3Heavy\">E-Bike Kit Must-Haves</span></h3>\r\n",
            "cq:responsive": {
              "jcr:primaryType": "nt:unstructured",
              "default": {
