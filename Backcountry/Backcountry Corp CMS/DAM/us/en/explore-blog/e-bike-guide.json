            "sling:resourceType": "baem/components/text",
            "jcr:lastModified": "Thu Aug 17 2023 19:34:18 GMT+0000",
            "jcr:primaryType": "nt:unstructured",
            "text": "<p>&nbsp;</p>\r\n<p>&nbsp;</p>\r\n<h3 style=\"text-align: center;\"><span class=\"title3Heavy\">E-Bike Features</span></h3>\r\n",
            "cq:responsive": {
              "jcr:primaryType": "nt:unstructured",
              "default": {
