            "text": "<h2 style=\"text-align: center;\">&nbsp;</h2>\r\n<h1 style=\"text-align: center;\">&nbsp;</h1>\r\n<h1 style=\"text-align: center;\"><span class=\"title3Heavy\">Matt Bluher's Story &amp; The Outdoor Sports Boom</span></h1>\r\n<h2 style=\"text-align: center;\"><span class=\"title2\"><i>Episode 3</i></span></h2>\r\n<h2 style=\"text-align: center;\">&nbsp;</h2>\r\n<h3 style=\"text-align: center;\">&nbsp;</h3>\r\n",
