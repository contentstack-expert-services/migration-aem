            "sling:resourceType": "baem/components/text",
            "jcr:lastModified": "Tue Aug 29 2023 21:26:37 GMT+0000",
            "jcr:primaryType": "nt:unstructured",
            "text": "<p>&lt;iframe style=&quot;border-radius:12px&quot; src=&quot;https://open.spotify.com/embed/episode/5gR5cWB2xhhpBPSn7rmh3q?utm_source=generator&quot; width=&quot;100%&quot; height=&quot;152&quot; frameBorder=&quot;0&quot; allowfullscreen=&quot;&quot; allow=&quot;autoplay; clipboard-write; encrypted-media; fullscreen; picture-in-picture&quot; loading=&quot;lazy&quot;&gt;&lt;/iframe&gt;</p>\r\n",
            "cq:responsive": {
