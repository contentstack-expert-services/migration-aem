pe":"nt:unstructured","text":"<h2 style=\"text-align: center;\">&nbsp;</h2>\r\n<h1 style=\"text-align: center;\">&nbsp;</h1>\r\n<h1 style=\"text-align: center;\"><span class=\"title3Heavy\">Julian Carr's Outdoor Sports Saga</span></h1>\r\n<h2 style=\"text-align: center;\"><span class=\"title2\"><i>Episode 8</i></span></h2>\r\n<h2 style=\"text-align: center;\">&nbsp;</h2>\r\n<h3 style=\"text-align: center;\">&nbsp;</h3>\r\n","cq:responsive":{"jcr:primaryType":"nt:unstructured","default":{"offset":"1","width":"10","jcr:primaryType":"nt:unstructured"}}},"text_1457080594":{"jcr:created":"Tue Aug 29 2023 21:40:41 GMT+0000","jcr:lastModifiedBy":"kzylstra@backcountry.com","jcr:createdBy":"kzylstra@backcountry.com","textIsRich":"true","sling:resourceType":"baem/components/text","jcr:lastModified":"Tue Aug 29 2023 21:41:00 GMT+0000","jcr:primaryType":"nt:unstructured","text":"<p>&lt;iframe style=&quot;border-radius:12px&quot; src=&quot;https://open.spotify.com/embed/episode/2kAjewX6OWlKatEhlrWjJB?utm_source=generator&quot; width=&quot;100%&quot; height=&quot;152&quot; frameBorder=&quot;0&quot; allowfullscreen=&quot;&quot; allow=&quot;autoplay; clipboard-write; encrypted-media; fullscreen; picture-in-picture&quot; loading=&quot;lazy&quot;&gt;&lt;/iframe&gt;</p>\r\n","cq:responsive":{"jcr:primaryType":"nt:unstructured","default":{"offset":"1","width":"10","jcr:primaryType":"nt:unstructured"}}},"text_498495224":{"jcr:created":"Wed Sep 13 2023 20:40:00 GMT+0000","jcr:lastModifiedBy":"kzylstra@backcountry.com","jcr:createdBy":"kzylstra@backcountry.com","textIsRich":"true","sling:resourceType":"baem/components/text","jcr:lastModified":"Wed Sep 13 2023 20:40:46 GMT+0000","jcr:primaryType":"nt:unstructured","text":"<p>&nbsp;</p>\r\n<p>&nbsp;</p>\r\n<p><i><span class=\"title1\">Show Notes</span></i></p>\r\n<p><span class=\"body1\">&nbsp;</span></p>\r\n<p><span class=\"body1\">Julian Carr is a decorated pro skier, businessman, and is known for hitting some of the biggest cliffs of all time on his skis. He’s also the founder of the Cirque Series which is the premiere mountain running series, of which Backcountry is a proud sponsor.&nbsp;&nbsp;</span></p>\r\n<p><span class=\"body1\">&nbsp;</span></p>\r\n<p><span class=\"body1\">Our podcast host Nils digs into Julian’s professional skiing career, touching on injuries, sponsorships, and graduating from 40ft to 220ft cliffs. The pair also talk through the conception of the Cirque Series and why these races mean so much to the greater mountain sports community.</span></p>\r\n<p><span class=\"body1\">&nbsp;</span></p>\r\n<p><span class=\"body1\"><a href=\"https://www.cirqueseries.com/\">Check out the Cirque Series</a></span></p>\r\n<p><span class=\"body1\"><a href=\"https://www.backcountry.com/explore/intro-to-trail-running\">Read our Intro To Trail Running Article</a></span></p>\r\n<p><span class=\"body1\">&nbsp;</span></p>\r\n<p><span class=\"body1\">Chapters With Times:</span></p>\r\n<p><span class=\"body1\">&nbsp;</span></p>\r\n<p><span class=\"body1\">0:00 – Intro&nbsp;</span></p>\r\n<p><span class=\"body1\">1:06 – Colorado vs. SLC Homebase&nbsp;&nbsp;</span></p>\r\n<p><span class=\"body1\">4:38 – Utah Upbringing&nbsp;</span></p>\r\n<p><span class=\"body1\">7:25 – Catching The Pro Skiing Bug&nbsp;</span></p>\r\n<p><span class=\"body1\">11:15 – Discipline &amp; Professionalism&nbsp;</span></p>\r\n<p><span class=\"body1\">13:34 – Balancing Skiing &amp; Business&nbsp;</span></p>\r\n<p><span class=\"body1\">17:40 – The Snowsports Athlete Timeline&nbsp;</span></p>\r\n<p><span class=\"body1\">20:46 – Discrete Clothing&nbsp;</span></p>\r\n<p><span class=\"body1\">23:28 – Cliff Jumping&nbsp;</span></p>\r\n<p><span class=\"body1\">31:24 – Femur Injury&nbsp;</span></p>\r\n<p><span class=\"body1\">36:40 – Big Cliff Progression&nbsp;</span></p>\r\n<p><span class=\"body1\">47:00 – Being Present&nbsp;</span></p>\r\n<p><span class=\"body1\">50:16 – Cirque Series Inspiration&nbsp;</span></p>\r\n<p><span class=\"body1\">1:03:20 – Race Breakdown&nbsp;</span></p>\r\n<p><span class=\"body1\">1:15:49 – DOJAMAT&nbsp;&nbsp;</span></p>\r\n<p><span class=\"body1\">1:18:08 – Outro&nbsp;</span></p>\r\n<p>&nbsp;</p>\r\n<p>&nbsp;</p>\r\n","cq:responsive":{"jcr:primaryType":"nt:unstructured","default":{"offset":"1","width":"10","jcr:primaryType":"nt:unstructured"}}},"customembed":{"embeddableResourceType":"baem/components/customembed/embeddable/youtube","jcr:created":"Mon Aug 28 2023 21:33:45 GMT+0000","jcr:lastModifiedBy":"kzylstra@backcountry.com","jcr:createdBy":"kzylstra@backcountry.com","textIsRich":"true","sling:resourceType":"baem/components/customembed","type":"embeddable","background_color":"white","youtubeVideoId":"v_J5tZJlqxI","jcr:lastModified":"Mon Aug 28 2023 21:35:06 GMT+0000","jcr:primaryType":"nt:unstructured"},"text":{"jcr:created":"Mon Aug 28 2023 21:35:15 GMT+0000","jcr:lastModifiedBy":"kzylstra@backcountry.com","jcr:createdBy":"kzylstra@backcountry.com","textIsRich":"true","sling:resourceType":"baem/components/text","jcr:lastModified":"Wed Sep 13 2023 20:39:35 GMT+0000","jcr:primaryType":"nt:unstructured","text":"<p>&nbsp;</p>\r\n<p>&nbsp;</p>\r\n<p><span class=\"body1\">The Backcountry Podcast (Backcountry.com) is a collection of conversations with influential members of the outdoor community, including our athletes and ambassadors. We dive deep into widespread topics from guests’ perspectives, ranging from film projects to personal pursuits and outdoor industry news. The Backcountry Podcast is hosted by Backcountry athlete, professional snowboarder, University of Utah Mechanical Engineering graduate, and all-around mountain guy, Nils Mindnich.</span></p>\r\n<p>&nbsp;</p>\r\n<p><span class=\"body1\"><a href=\"https://podcasts.apple.com/us/podcast/ep-8-julian-carrs-outdoor-sports-saga/id1677586211?i=1000619596958\">Listen on Apple Podcasts</a></span></p>\r\n<p><span class=\"body1\"><a href=\"https://open.spotify.com/episode/2kAjewX6OWlKatEhlrWjJB?si=UtttpQWcSOCiv5RMT9tp5A\">Listen on Spotify</a></span></p>\r\n<p>&nbsp;</p>\r\n<p><span class=\"body1\">Check out the products featured &amp; Julian's sponsors here:</span></p>\r\n<p>&nbsp;</p>\r\n<p><span class=\"body1\"><a href=\"https://www.backcountry.com/on?tabNav=0\">Shop All On Running Shoes &amp; Apparel</a></span></p>\r\n<p><span class=\"body1\"><a href=\"https://www.backcountry.com/rab?tabNav=0\">Shop everything Rab</a></span><br>\r\n</p>\r\n<p><span class=\"body1\"><a href=\"https://www.backcountry.com/icelantic\">Shop Icelantic Skis</a></span></p>\r\n<p><span class=\"body1\"><a href=\"https://www.backcountry.com/gordini\">Shop everything Gordini</a></span></p>\r\n<p><span class=\"body1\"><a href=\"https://www.backcountry.com/backcountry-ski\">Shop all Backcountry skiing apparel</a></span></p>\r\n<p><span class=\"body1\"><a href=\"https://www.backcountry.com/rc/trail-running-shoes\">Shop all trail running shoes</a></span></p>\r\n<p>&nbsp;</p>\r\n<p><span class=\"body1\">Connect with a Gearhead: Call 1-800-409-4502 or chat with us online.</span></p>\r\n<p><span class=\"body1\">&nbsp;</span></p>\r\n<p><span class=\"body1\">What makes Backcountry the best place to gear up? One-on-one Gearhead expertise. The greatest selection of outdoor gear, apparel, and accessories out there. And the tools to elevate your entire outdoor experience, from training smarter and sending harder to toasting at après. Harnessing the insights of our Gearheads, we also build our own products for ski, camp, MTB, fly fish, and more pursuits, including exclusive collabs with industry leaders. Backcountry is the place to Venture Beyond.</span></p>\r\n<p>&nbsp;</p>\r\n","cq:responsive":{"jcr:primaryType":"nt:unstructured","default":{"offset":"1","width":"10","jcr:primaryType":"nt:unstructured"}}},"spacer":{"jcr:created":"Mon Sep 25 2023 18:42:56 GMT+0000","jcr:lastModifiedBy":"kzylstra@backcountry.com","jcr:createdBy":"kzylstra@backcountry.com","sling:resourceType":"baem/components/spacer","pixels_default":"24","pixels_large":"24","pixels_medium":"24","jcr:lastModified":"Mon Sep 25 2023 18:43:03 GMT+0000","pixels_small":"24","jcr:primaryType":"nt:unstructured","pixels_base":"24"},"button_copy":{"jcr:created":"Mon Sep 25 2023 18:36:07 GMT+0000","jcr:lastModifiedBy":"kzylstra@backcountry.com","jcr:createdBy":"kzylstra@backcountry.com","link":"https://www.backcountry.com/service/backcountry-podcast","sling:resourceType":"baem/components/button","jcr:title":"All Podcast Episodes","jcr:lastModified":"Mon Sep 25 2023 18:37:40 GMT+0000","variant":"brand","jcr:primaryType":"nt:unstructured","alignment":"center"}}}}}}