            "item_1699895069049": {
              "cq:panelTitle": "What is a dad hat?",
              "sling:resourceType": "baem/components/container",
              "jcr:primaryType": "nt:unstructured",
              "text": {
                "jcr:created": "Mon Nov 13 2023 17:04:44 GMT+0000",
                "jcr:lastModifiedBy": "steph.briggs@backcountry.com",
                "jcr:createdBy": "steph.briggs@backcountry.com",
                "textIsRich": "true",
                "sling:resourceType": "baem/components/text",
                "jcr:lastModified": "Mon Nov 13 2023 17:05:33 GMT+0000",
                "jcr:primaryType": "nt:unstructured",
                "text": "<p>Dad hats are a more casual version of a <a href=\"https://www.backcountry.com/mens-baseball-hats\">baseball hat</a>.</p>\r\n"
              }
            },
            "item_2": {
              "layout": "responsiveGrid",
              "cq:panelTitle": "How many panels is a dad hat?",
              "sling:resourceType": "baem/components/container",
              "jcr:primaryType": "nt:unstructured",
              "jcr:title": "Item 2",
              "text": {
                "jcr:created": "Mon Nov 13 2023 17:04:46 GMT+0000",
                "jcr:lastModifiedBy": "steph.briggs@backcountry.com",
                "jcr:createdBy": "steph.briggs@backcountry.com",
                "textIsRich": "true",
                "sling:resourceType": "baem/components/text",
                "jcr:lastModified": "Mon Nov 13 2023 17:05:51 GMT+0000",
                "jcr:primaryType": "nt:unstructured",
                "text": "<p>A dad hat has 6 panels with an unstructured fit.</p>\r\n"
              }
            },
            "item_1699894984420": {
              "cq:panelTitle": "What color hat should I wear?",
              "sling:resourceType": "baem/components/container",
              "jcr:primaryType": "nt:unstructured",
              "text": {
                "jcr:created": "Mon Nov 13 2023 17:04:48 GMT+0000",
                "jcr:lastModifiedBy": "steph.briggs@backcountry.com",
                "jcr:createdBy": "steph.briggs@backcountry.com",
                "textIsRich": "true",
                "sling:resourceType": "baem/components/text",
                "jcr:lastModified": "Mon Nov 13 2023 17:05:59 GMT+0000",
                "jcr:primaryType": "nt:unstructured",
                "text": "<p>If you are out in the sun, a light-colored hat, like white and tan, will reflect sunlight to help keep you cool. If you are susceptible to dirt and grime while wearing your hat, a dark color, like black and navy, will help hide stains. </p>\r\n"
              }
            },
            "item_1699894986187": {
              "cq:panelTitle": "Do hat and shoes have to match?",
              "sling:resourceType": "baem/components/container",
              "jcr:primaryType": "nt:unstructured",
              "text": {
                "jcr:created": "Mon Nov 13 2023 17:04:50 GMT+0000",
                "jcr:lastModifiedBy": "steph.briggs@backcountry.com",
                "jcr:createdBy": "steph.briggs@backcountry.com",
                "textIsRich": "true",
                "sling:resourceType": "baem/components/text",
                "jcr:lastModified": "Mon Nov 13 2023 17:06:31 GMT+0000",
                "jcr:primaryType": "nt:unstructured",
                "text": "<p>Your hat and <a href=\"https://www.backcountry.com/mens-footwear\">shoes</a> do not have to match, but they can if you want them to!</p>\r\n"
              }
            }
          }
        }
